$1.$2Error = function(message) {
  var error = new Error(message);
  error.name = this.name = 'IdFly.Js.$1.$2Error';
  this.message = error.message;
  this.stack = error.stack;
};

$1.$2Error.prototype = Error.prototype;
$1.$2Error.prototype.constructor = $1.$2Error;
